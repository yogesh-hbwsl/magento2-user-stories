<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <smartystreets>
            <validation>
                <auth_id backend_model="Magento\Config\Model\Config\Backend\Encrypted" />
                <auth_token backend_model="Magento\Config\Model\Config\Backend\Encrypted" />
            </validation>
            <autocomplete>
                <website_key backend_model="Magento\Config\Model\Config\Backend\Encrypted" />
            </autocomplete>
        </smartystreets>
    </default>
</config>
