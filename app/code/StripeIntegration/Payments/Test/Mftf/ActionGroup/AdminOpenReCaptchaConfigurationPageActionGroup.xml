<?xml version="1.0" encoding="UTF-8"?>
<actionGroups xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:noNamespaceSchemaLocation="urn:magento:mftf:Test/etc/actionGroupSchema.xsd">
    <actionGroup name="AdminOpenReCaptchaConfigurationPageActionGroup">
        <annotations>
            <description>Open reCaptcha configuration page.</description>
        </annotations>

        <amOnPage url="{{AdminConfigurationReCaptchaPage.url}}" stepKey="openReCaptchaConfigurationPage"/>
